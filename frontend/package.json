{
  "name": "@devpont/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx .",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache 'src/**/*.scss'",
    "stylecheck": "sass --load-path=node_modules --load-path=src/styles --update -q ./src:./node_modules/.cache/.stylecheckcache -q",
    "preview": "vite preview",
    "types": "tsc --noEmit --project ./tsconfig.json && tsc --noEmit --project ./tsconfig.node.json",
    "prettify": "prettier --log-level warn --cache --write '{src/**/*,*}.{js,ts,tsx,json,css,scss,yml}' '!**/{node_modules, dist}/**/*'"
  },
  "dependencies": {
    "@devpont/backend": "workspace:*",
    "@tanstack/react-query": "^5.79.0",
    "@trpc/client": "^11.1.4",
    "@trpc/react-query": "^11.1.4",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "formik-validator-zod": "^2.2.0",
    "include-media": "^2.0.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.1.0",
    "react-infinite-scroller": "^1.2.6",
    "react-router-dom": "^7.6.1",
    "reset-css": "^5.0.2",
    "sass": "^1.89.1",
    "superjson": "^2.2.2",
    "usehooks-ts": "^3.1.1",
    "zod": "^3.25.49"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.17",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-infinite-scroller": "^1.2.5",
    "@vitejs/plugin-react": "^4.3.4",
    "globals": "^15.15.0",
    "stylelint": "^16.20.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "vite": "^6.2.0"
  },
  "packageManager": "pnpm@10.7.1+sha512.2d92c86b7928dc8284f53494fb4201f983da65f0fb4f0d40baafa5cf628fa31dae3e5968f12466f17df7e97310e30f343a648baea1b9b350685dafafffdf5808"
}
